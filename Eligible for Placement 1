import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    String st= sc.nextLine();
    int a = sc.nextInt();
    int b = sc.nextInt();
    System.out.println("Name of the Student:" + st);
    if (a == 1 && b>70)
    {
    System.out.println(st + " is Eligible for Placement");
    } 
    else if (a==1||a==2 && b>75)
    {
    System.out.println(st + " is Eligible for Placement");
    } 
    else 
    {
    System.out.println(st + " is Not Eligible for Placement");
    }

    }

}
